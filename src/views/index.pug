extends layout

block content
	.container
		h1(class="text-center")= title
		p <b>Welcome to #{title}</b>
		p Links:

		ul
		each value, name in {"DOM test":"dom/test.html", "events":"dom/events.html","events in details":"dom/detail-events.html","forms":"dom/form"}
			li <a href="#{value}">#{name}</a>